{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "remix-app#build": {
      "dependsOn": ["^build"],
      "outputs": ["public/build/**"]
    },
    "remix-app#start": {
      "dependsOn": ["^build"],
      "outputs": ["public/build/**"]
    },
    "start": {
      "dependsOn": ["^build"],
      "outputs": ["public/build/**"]
    },
    "db:migrate:deploy": {
      "outputs": []
    },
    "db:migrate:dev": {
      "outputs": []
    },
    "db:push": {
      "outputs": []
    },
    "db:seed": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "generate": {
      "dependsOn": ["^generate"]
    },
    "lint": {
      "outputs": []
    },
    "docker:build": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "outputs": []
    }
  },
  "globalDependencies": [".env", "$NODE_ENV", "$REMIX_APP_PORT", "$FLY_REGION", "$PRIMARY_REGION"]
}
